{
    "name": "duyler/io",
    "type": "library",
    "description": "Duyler async library",
    "license": "MIT",
    "keywords": [
        "async",
        "fiber",
        "io",
        "parallel"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/en/guilde/io/"
    },
    "require": {
        "php": "^8.3",
        "ext-parallel": "*",
        "duyler/di": "dev-main",
        "duyler/builder": "dev-main",
        "duyler/event-bus": "dev-main",
        "duyler/database": "dev-main",
        "amphp/serialization": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "yiisoft/hydrator": "^1.6",
        "loophp/collection": "^7.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "psr/container": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.30",
        "squizlabs/php_codesniffer": "^4.0",
        "vimeo/psalm": "^6.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Duyler\\IO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\IO\\Test\\": ["tests/"]
        }
    }
}
